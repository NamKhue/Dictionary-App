<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<DialogPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml" fx:controller="features.controller.EditWordController">
   <headerText>
   </headerText>
   <content>
      <GridPane hgap="10" vgap="10">
         <Label fx:id="Confirm" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
         <Label text="New Word: " GridPane.columnIndex="0" GridPane.rowIndex="1"/>

         <TextField fx:id="editWord" prefHeight="26.0" prefWidth="243.0" GridPane.columnIndex="1"
                    GridPane.rowIndex="1"/>
         <columnConstraints>
            <ColumnConstraints/>
            <ColumnConstraints/>
         </columnConstraints>
         <rowConstraints>
            <RowConstraints/>
            <RowConstraints/>
         </rowConstraints>

      </GridPane>
   </content>

</DialogPane>